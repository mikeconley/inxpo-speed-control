{
  "manifest_version": 2,
  "name": "INXPO Speed Control",
  "version": "0.2",
  "description": "Adds a speed control to INXPO videos.",
  "applications": {
    "gecko": {
      "id": "inxpo-speed-control@mikeconley.ca",
      "strict_min_version": "52.0"
    }
  },
  "permissions": [],

  "content_scripts": [
    {
      "matches": ["https://onlinexperiences.com/scripts/Server.nxp*"],
      "js": ["content.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}

